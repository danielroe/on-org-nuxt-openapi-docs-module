    "url": "^0.11.0",
    "vue": "^3.2.47",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.10",
    "flowbite": "^1.6.4",
    "request-templater": "^1.1.2",
    "tailwindcss": "^3.2.7"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  }
